{
  "name": "ai-psx-gpt5",
  "version": "0.0.0",
  "private": true,
  "description": "Headless, accuracy-focused PlayStation 1 emulator in TypeScript with automated testing and Chrome host.",
  "license": "MIT",
  "author": "",
  "type": "module",
  "scripts": {
    "build": "npm run -ws build",
    "test": "vitest --run",
    "test:coverage": "vitest --run --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.cjs,.mjs",
    "format": "prettier -w .",
    "e2e": "echo 'e2e placeholder'"
  },
  "workspaces": [
    "packages/*",
    "apps/*",
    "tools/*",
    "tests/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.2.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  }
}
